<script type="text/jscript">
    $(document).ready(function () {
        $('.back').click(function () {
            parent.history.back();
            return false;
        });
    });

    function goBack() {
        window.history.back();
    }

    //ADDED CODE BY SHRADDHA ON 02 MARCH 2017 FOR ALERT POPUP START
    $("#CreateCompanyBtn").click(function () {
        //alert("Click");
        var $form = $(this).closest('form');
        $('#Companydiv').modal({ backdrop: 'static', keyboard: false })
                .one('click', '#AddBtn', function () {

                    //-- ADDED BY SHALAKA ON 21st June 2017 FOR PROGRESS RING
                   // showloading();

                    $("#AddCompanyForm").submit();

                });

    });


    //ADDED CODE BY SHRADDHA ON 02 MARCH 2017 FOR ALERT POPUP END
</script>
