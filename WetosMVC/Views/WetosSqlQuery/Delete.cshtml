            <div class="row">
                <div class="col-xs-12 col-md-12 col-lg-12">

                    <div class="box">
                    
                        <!-- /.box-header -->
                        <div class="box-body">
                       
                            @using (Html.BeginForm("Delete", "WetosRateOfInterestMaster", FormMethod.Post, new { id = "AddCompanyForm", enctype = "multipart/form-data" }))
                            {
                          
                                 <div style="color: Red; font-size: 12px">
                                    @Html.ValidationSummary(true)
                                </div>

                                @Html.HiddenFor(model=>model.ROIID)
                                <div style="color: Red; font-size: 12px">
                                    @Html.ValidationMessage("Error")
                                </div>
        
                                <div class="row">
                                    <div class="editor-label col-md-2">
                                        Comapny Name  <span style="color: Red; font-size: 18px">*</span>
                                    </div>
                                    <div class="editor-field col-md-5">
                                   
                                    <div class="input-group date">
                                       <div class="input-group-addon">
                                        <i  class="fa fa-map-marker"></i>
                                        </div> 
                                      
                                       @Html.DropDownListFor(model => model.CompanyId, (IEnumerable<SelectListItem>)ViewBag.CompanyListView, "Select", new { @class = "form-control select2", style = "width:250px" ,@disabled="disabled"})
                                       
                                        </div>
                                         
                                           <div  style="color: Red; font-size: 12px">
                                        @Html.ValidationMessageFor(model => model.CompanyId)
                                        </div>

                                        </div>
                                </div>
                              
                                <div class="row">
                                    <div class="form-group">
                                        <div class="editor-label col-md-2">
                                            Branch Name <span style="color: Red; font-size: 18px">*</span>
                                        </div>
                                        <div class="editor-field col-md-5">
                                      
                                        <div class="input-group date">
                                       <div class="input-group-addon">
                                        <i class="fa fa-suitcase" aria-hidden="true"></i>
                                        </div> 

                                            @Html.DropDownListFor(model => model.BranchId, (IEnumerable<SelectListItem>)ViewBag.BranchListView, "Select", new { @class = "form-control select2", style = "width:250px", @disabled = "disabled" })
                                            </div>
                                         
                                           <div style="color: Red; font-size: 12px">
                                                @Html.ValidationMessageFor(model => model.BranchId)
                                            </div>
                                        </div>
                                    </div>
                                </div>
                               
                                <div class="row">
                                    <div class="editor-label col-md-2">
                                        Department <span style="color: Red; font-size: 18px">*</span>
                                    </div>
                                    <div class="editor-field col-md-5">
                                     
                                        <div class="input-group date">
                                       <div class="input-group-addon">
                                       <i class="fa fa-building-o" aria-hidden="true"></i>
                                        </div> 

                                        @Html.DropDownListFor(model => model.DepartmentId, (IEnumerable<SelectListItem>)ViewBag.DepartmentListView, "Select", new { @class = "form-control select2", @style = "width:250px", @disabled = "disabled" })
                                        <div  style="color: Red; font-size: 12px">
                                            @Html.ValidationMessageFor(model => model.DepartmentId)
                                        </div>
                                        </div>
                                    </div>
                                </div>
                               
                                <div class="row">
                                    <div class="editor-label col-md-2">
                                        Amount1  <span style="color: Red; font-size: 18px">*</span>
                                    </div>
                                    <div class="editor-field col-md-5">
                                    
                                        <div class="input-group date">
                                       <div class="input-group-addon">
                                        <i class="fa fa-building-o" aria-hidden="true"></i>
                                        </div> 
                                         @Html.TextBoxFor(model => model.RangeOneAmount, new { @class = "form-control", @style = "width:250px", @readonly = "readonly" })
                                          @Html.ValidationMessageFor(model => model.RangeOneAmount)
                                        </div>
                                    </div>
                                </div>
                                
