                                          @Html.ValidationMessageFor(model => model.RangeTwoAmount)
                                        </div>
                                    </div>
                                </div>
                                
                                <div class="row">
                                    <div class="editor-label col-md-2">
                                        Rate Of Interest <span style="color: Red; font-size: 18px">*</span>
                                    </div>
                                    <div class="editor-field col-md-5">
                                        <div class="input-group date">
                                       <div class="input-group-addon">
                                        <i class="fa fa-building-o" aria-hidden="true"></i>
                                        </div> 
                                       

                                      
                                        @Html.TextBoxFor(model => model.RateOfInterestValue, new { @class = "form-control", @style = "width:250px", @readonly = "readonly" })
