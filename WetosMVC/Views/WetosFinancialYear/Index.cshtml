@model IEnumerable<WetosDB.FinancialYear>
@{
    ViewBag.Title = "Financial Year List";
}
@System.Web.Optimization.Styles.Render("~/Content/DataTable")
@System.Web.Optimization.Scripts.Render("~/bundles/DataTable")
<div class="page-content-wrapper" id="page_content">
    <div class="page-content">
        <section class="content-header">
      <h1>
Financial Year
        <small>List</small>
         
         <span data-toggle="tooltip" title="Add to Nev Menu" style="float:right;font-weight:800" onclick="ÖnAddHoldSalaryClick();">
         @Html.ActionLink(" ", "Create", new { }, new { @class = "fa fa-plus-circle", title = "Add to Financial Year", style = "color:#428bca;font-size:19px" })</span>
      </h1>
    
       <div class="row">
                <div class="col-md-12">
                    <ul class="page-breadcrumb breadcrumb">
                        <li>
                            <i class="fa fa-home"></i>
                            <a href="@Url.Action("Index", "Home")">Home </a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li>
                         <a href="#">WetosFinancialYear</a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li>
                            <a href="#">Finanacial Year</a>
                        </li>
                        <li class="pull-right">
                            <div id="dashboard-report" data-toggle="tooltip" title="Today's date">
                                <span style="cursor:pointer" class="back " data-toggle="tooltip" title="Back" id="goback"><i class="fa fa-arrow-left"></i></span>
                                <i class="fa fa-calendar"></i>
                                <span>@DateTime.Now.ToLongDateString()</span>                           
                            </div>
                        </li>              
                    </ul>
                </div>
            </div>

     

    </section>
        <section class="content">
      <div class="row">
        <div class="col-xs-12 col-md-12 col-lg-12">
          <div class="box">
          
            <!-- /.box-header -->
            <div class="box-body">
           @* <p>@Html.ActionLink("Add Financial Year", "Create")</p>*@
<table id="FYName" class="table table-bordered table-striped">
<thead>
    <tr>
        
        <td>
            Financial Name
        </td>
        <td>
            Start Date
        </td>
        <td>
            End Date
        </td>
       @* <td>
            Company
        </td>
        <td>
            Branch
        </td>*@
        <td>Action</td>
    </tr>
    </thead>
    <tbody>
@foreach (var item in Model)
{
    <tr>
        
        <td>
            @item.FinancialName
        </td>
        <td>
        
         @item.StartDate.ToString("dd-MMM-yyyy")
           @*@item.StartOfFinancialYear*@
        </td>
        <td>
            @item.EndDate.ToString("dd-MMM-yyyy")
        </td>
       
        <td>
           <span data-toggle="tooltip" data-toggle="tooltip" title="Edit">
                                                @Html.ActionLink(" ", "Edit", new { id = item.FinancialYearId }, new { @class = "fa fa-pencil-square-o" })</span> 
          |<span data-toggle="tooltip" data-toggle="tooltip" title="Edit">
                                                @Html.ActionLink(" ", "Delete", new { id = item.FinancialYearId }, new { @class = "fa fa-trash" })</span>    
           
        </td>
    </tr>
}
</tbody>
<tfoot></tfoot>
</table>
</div>
</div>
</div>
</section>
    </div>
</div>
<script type="text/jscript">
    $(document).ready(function () {
        $('.back').click(function () {
            parent.history.back();
            return false;
        });
    });

    function goBack() {
        window.history.back();
    }
</script>
<script type="text/jscript">
    $(function () {

        $("#FYName").DataTable();

    });
</script>
