@model IEnumerable<WetosDB.CompOffApplication>

@{
    ViewBag.Title = "Comp Off Application";
}

<h2>Comp Off Application</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped">
    <tr>
        
        <th style = "font-weight:100">
            EMPLOYEE ID
        </th>
        <th style = "font-weight:100">
            FROM DATE
        </th>
        <th style = "font-weight:100">
            FROM DATE STATUS
        </th>
        @*<th>*@
           @* TODATE
        </th>
        <th>
            TODATESTATUS
        </th>*@
       @* <th>
            PURPOSE
        </th>*@
        @*<th>
            STATUS
        </th>
        <th>
            REJECTREASON
        </th>*@
        <th style = "font-weight:100">
            EXTRAS HOURS DATE
        </th>
       @* <th>
            BRANCHID
        </th>
        <th>
            COMPANYID
        </th>*@
        <th style = "font-weight:100"> ACTION </th>
    </tr>

@foreach (var item in Model) 
{
    <tr>
       
        <td>
            @item.EmployeeId
        </td>
        <td>
            @String.Format("{0:g}", item.FromDate)
        </td>
        <td>
            @item.FromDateStatus
        </td>
        @*<td>
            @String.Format("{0:g}", item.ToDate)
        </td>
        <td>
            @item.ToDateStatus
        </td>*@
       @* <td>
            @item.Purpose
        </td>*@
        @*<td>
            @item.Status
        </td>
        <td>
            @item.RejectReason
        </td>*@
        <td>
            @item.ExtrasHoursDate
        </td>
        @*<td>
            @item.BranchId
        </td>
        <td>
            @item.CompanyId
        </td>*@
         <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CompOffId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CompOffId })
            @*@Html.ActionLink("Delete", "Delete", new { id=item.CompOffId })*@
        </td>
    </tr>
}

</table>

