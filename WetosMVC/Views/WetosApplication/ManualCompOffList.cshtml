@model IEnumerable<WetosDB.SP_GetManualCompOffList_Result>
@{
    ViewBag.Title = "ManualCompOffList";
}
<link href="~/Content/css/manual_compoff_list1.css" rel="stylesheet" />
<link href="~/Content/allcss/allcss.css" rel="stylesheet" />
 
<div class="page-content-wrapper" id="page_content">
    <div class="page-content">
        <section class="content-header">
            <div class="row page-breadcrumb breadcrumb ed_breadcrumb ed_breadcrumb_row">
                <div class="col-sm-6 ed_breadcrumb_div">
                    <ul>
                        <li class="li_margin1">
                            <img src="~/Content/images/icon/home_1.png" class="home_width_height" />
                            <a href="@Url.Action("Index", "Home")">Home</a><i class="fa fa-angle-right"></i>

                        </li>
                        <li class="li_margin"> 
                         <a href="#">Application</a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li class="li_margin">
                            <a href="@Url.Action("ManualCompOffList", "WetosApplication")">Manual Comp Off </a>
                        </li>
                        </ul>
                </div>
                <div class="col-sm-6">
                    <ul style="float: right;">
                        <li class="">
                            <div id="dashboard-report margin_left" data-toggle="tooltip" title="Today's date">
                                <img src="~/Content/images/icon/imageedit_42.png" class="icon_width_height" />
                                <span>
                                    @DateTime.Now.ToLongDateString()
                                </span>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
            <h1 class="margin_left_h1">Manual Comp Off
                <small>List</small>
                 <span class="btn blue" data-toggle="tooltip" title="Apply Manual Comp Off" style="float:right;font-weight:800" onclick="OnAddCompanyClick();">@Html.ActionLink(" ", "ManualCompOffApplicationIndex", new { }, new { @class = "fa fa-plus-square", style = "color:white" })</span>
            </h1>
           
        </section>
        <section class="content">
           
            
            <div class="row">
                <div class="col-xs-12">
                    <div class="box">
              
                            <div class="box-body">
                           
                                <table id="ManualCOMaster" class="table table-bordered table-striped">  @*Added by Pushkar For datatable on 25 OCT 2016*@
                                    <thead>
    <tr>
        @*<th></th>*@
        
        <th>
           Emp. Name
        </th>
        <th>
            FromDate
        </th>
        <th>
            Extra Working Hours
        </th>
        <th>
            CompOff Purpose
        </th>
        <th>
            Status
        </th>
        @*<th>
            CompOffBalance
        </th>*@
        @*<th>
            BranchId
        </th>
        <th>
            CompanyId
        </th>*@
        <th>
            Reject Reason
        </th>
        <th>Action</th>
    </tr>
    </thead>
                                    <tbody>
@foreach (var item in Model)
{
    <tr>
      @*  <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ManualCompOffId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ManualCompOffId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ManualCompOffId })
        </td>*@
       @* <td>
            @item.EmployeeId
        </td>*@
        <td>
            @item.EmployeeCode - @item.FirstName @item.MiddleName @item.LastName
        </td>
        <td>
            @item.FromDate
        </td>
        <td>
            @String.Format("{0:g}", item.ExtraWorkingHrs)
        </td>
        <td>
            @item.CompOffPurpose
        </td>
        <td>
        
            @item.StatusName
        </td>
       @* <td>
            @String.Format("{0:F}", item.CompOffBalance)
        </td>*@
       @* <td>
            @item.BranchId
        </td>
        <td>
            @item.CompanyId
        </td>*@
        <td>
            @item.RejectReason
        </td>
          <td>
                                              <span data-toggle="tooltip" title="Edit">
                                                @Html.ActionLink(" ", "ManualCompOffEdit", new { id = item.ManualCompOffId }, new { @class = "fa fa-pencil-square-o" })</span> |
                                                @*@Html.ActionLink("Details", "Details", new { id = item.CompanyId }) |*@
                                                <span data-toggle="tooltip" title="Delete">
                                                @Html.ActionLink(" ", "Delete", new { id = item.ManualCompOffId }, new { @class = "fa fa-trash" })
                                                </span>
                                            </td>
    </tr>
}
  </tbody>
                                <tfoot></tfoot>
                            </table>
                        </div>
                        <!-- /.box-body -->
                    </div>                    
                    <!-- /.box -->
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </section>
    </div>
</div>
