                            </div>
                            @* <span style="float:left">@Html.ActionLink("Add New Location", "Create")</span>*@
                        </li>
                    </ul>
                </div>
            </div>
    </section>
        @*<div align="Right">
<p align="Right">@Html.ActionLink("Back to Dashboard", "Dashboard", "Salary")</p> </div>*@
        <section class="content">
     
          <div class="box">
         
            <!-- /.box-header -->
            <div class="box-body">
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    
    @*<div class="display-label">Grade Id</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.GradeId)
    </div>*@

    <div class="row">
    <div class="display-label col-md-4">Company Name</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CompanyName)
    </div></div> 

    <div class="row">
    <div class="display-label col-md-4">Branch Name</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.BranchName)
    </div></div> 

    <div class="row">
    <div class="display-label col-md-4">Employee Type</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.EmployeeTypeName)
    </div></div> 

    <div class="row">
    <div class="display-label col-md-4">Grade Name</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.GradeName)
    </div></div> 

<div>
  <span style="cursor:pointer; float:left" title = "Click to edit Grade" class="btn btn-default" >@Html.ActionLink("Edit", "Edit", new { id = Model.GradeId })</span>
             <span  class="btn btn-default" style="cursor:pointer; float:left; margin-left:10px" title = "Click to go back to Grade list">@Html.ActionLink("Back to Grade list", "Index")</span>
   
</div>
<br />
   
}
 </div>
    </div>
    </section>
    </div>
</div>
<script type="text/jscript">
    $(document).ready(function () {
        $('.back').click(function () {
            parent.history.back();
            return false;
        });
    });

    function goBack() {
        window.history.back();
    }

</script>
