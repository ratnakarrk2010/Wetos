@model WetosDB.VwShiftGroupDetail
@{
    ViewBag.Title = "Details";
}
<div>
    <section class="content-header">

        <ol class="breadcrumb">
            <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Masters</li>
            <li><a href="@Url.Action("Index","WetosShiftGroup")"><i class="fa fa-arrow-left"></i> Shift Group</a></li>
            <li><a href="#">Details</a></li>

        </ol>
        <h1 class="margin_left_h1">
            Shift Group
            <small>Details</small>
        </h1>
        @*Added by Pushkar For back breadcrumb on 24 OCT 2016----------Start*@
        <div>
            <span style="float:right"><a href="#" id="goback" class="fa fa-arrow-left back"></a></span>
            @* <span style="float:left">@Html.ActionLink("Add New Location", "Create")</span>*@

        </div>
        <div style="clear: both;"></div>

        @*Added by Pushkar For back breadcrumb on 24 OCT 2016-----------End*@

    </section>
</div>
<section class="content">

    <div class="box">
        <div class="box-header">
            @*<h3 class="box-title">Shift Group Details</h3>*@
        </div>

        <!-- /.box-header -->
        <div class="box-body">
            <div class="row">
                <div class="col-md-4">Rotation Code</div>
                <div class="col-md-8">
                    @Html.DisplayFor(model => model.RotationCode)
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-4">Rotation Name</div>
                <div class="col-md-8">
                    @Html.DisplayFor(model => model.RotationName)
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-4">Rotation On Day Basis</div>
                <div class="col-md-8">
                    @Html.DisplayFor(model => model.RotationOnDayBasis)
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-4">Rotation On Month Basis</div>
                <div class="col-md-8">
                    @Html.DisplayFor(model => model.RotationOnMonthBasis)
                </div>
            </div>

            <br />

            <div class="row">
                <div class="col-md-4">Rotation On Weekly Basis</div>
                <div class="col-md-8">
                    @Html.DisplayFor(model => model.RotationOnWeeklyBasis)
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-4">Shifts Changes</div>
                <div class="col-md-8">
                    @Html.DisplayFor(model => model.ShiftsChanges)
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-4">Branch Name</div>
                <div class="col-md-8">
                    @Html.DisplayFor(model => model.BranchName)
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-4">Company Name</div>
                <div class="col-md-8">
                    @Html.DisplayFor(model => model.CompanyName)
                </div>
            </div>

            <br />

            <p class="margin_left_save">
                @Html.ActionLink("Edit", "Edit", new { id = Model.RotationId }) |
                @Html.ActionLink("Back to List", "Index")
            </p>

            <!-- /.box-body -->
        </div>
        <!-- /.box -->
        <!-- /.box -->
    </div>
    <!-- /.col -->
   
    <!-- /.row -->
</section>


















