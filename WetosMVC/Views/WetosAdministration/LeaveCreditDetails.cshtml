                <div class="box-body">


                    <div class="row">
                        <div class="display-label col-md-4">Employee</div>
                        <div class="display-field">
                            @Html.DisplayFor(model => model.EmployeeId)
                        </div>
                    </div>

                    <br />
                    <div class="row">
                        <div class="display-label col-md-4">Financial Year</div>
                        <div class="display-field">
                            @Html.DisplayFor(model => model.FinancialYearId)
                        </div>
                    </div>

                    <br />
                    <div class="row">
                        <div class="display-label col-md-4">Leave Type</div>
                        <div class="display-field">
                            @Html.DisplayFor(model => model.LeaveType)
                        </div>
                    </div>

                    <br />
                    <div class="row">
                        <div class="display-label col-md-4">Availed</div>
                        <div class="display-field">
