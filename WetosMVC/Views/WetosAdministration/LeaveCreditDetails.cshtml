                        </div>
                    </div>

                    <br />
                    <div class="row">
                        <div class="display-label col-md-4">Company</div>
                        <div class="display-field">
                            @Html.DisplayFor(model => model.CompanyId)
                        </div>
                    </div>

                    <br />
                    <div class="row">
                        <div class="display-label col-md-4">Branch</div>
                        <div class="display-field">
                            @Html.DisplayFor(model => model.BranchId)
                        </div>
                    </div>

                    <br />

                    <p>
                        @Html.ActionLink("Edit", "LeaveCreditEdit", new { id = Model.LeaveCreditId }) |
                        @Html.ActionLink("Back to List", "LeaveCreditIndex")
                    </p>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->
                <!-- /.box -->
