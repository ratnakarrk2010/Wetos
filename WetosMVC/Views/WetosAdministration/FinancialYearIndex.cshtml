@model IEnumerable<WetosDB.SP_GetFinancialYearDataList_Result>
@{
    ViewBag.Title = "Financial Year List";
}
<link href="~/Content/css/financial_year_index1.css" rel="stylesheet" />
<link href="~/Content/allcss/allcss.css" rel="stylesheet" />
<div class="page-content-wrapper" id="page_content">
    <div class="page-content">
        <section class="content-header">
            <div class="row page-breadcrumb breadcrumb ed_breadcrumb">
                <div class="col-sm-6">
                    <ul>
                        <li class="li_margin1">
                            <img src="~/Content/images/icon/home_1.png" class="home_width_height" />
                            <a href="@Url.Action("Index", "Home")">Home</a><i class="fa fa-angle-right"></i>

                        </li>
                       <li class="li_margin">
                         <a href="#">Administration</a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li class="li_margin">
                            <a href="#">Finanacial Year</a>
                        </li>
                    </ul>
                </div>
                <div class="col-sm-6">
                    <ul style="float: right;">
                        <li class="">
                            <div id="dashboard-report margin_left" data-toggle="tooltip" title="Today's date">
                                <img src="~/Content/images/icon/imageedit_42.png" class="icon_width_height" />
                                <span>
                                    @DateTime.Now.ToLongDateString()
                                </span>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
             <h1>
         Financial Year
        <small>List</small>
         <span style="float:right;font-weight:800" title="Create">
         @Html.ActionLink(" ", "FinancialYearCreate", new { }, new { @class = "fa fa-plus-circle", title = "Add Financial Year", style = "color:#428bca;font-size:19px" })</span>
      </h1>

      

       @*Added by Pushkar For back breadcrumb on 24 OCT 2016----------Start*@
     @* <div>
      <span style="float: left" class="btn btn-default">@Html.ActionLink("Add Financial Year", "FinancialYearCreate", "WetosAdministration")</span>

      </div>
      <div style="clear: both;"></div>*@

       @*Added by Pushkar For back breadcrumb on 24 OCT 2016-----------End*@

    </section>
        <section class="content">
      <div class="row">
        <div class="col-xs-12 col-md-12 col-lg-12">
          <div class="box">
           @* <div class="box-header">
              
            </div>*@
            <!-- /.box-header -->
            <div class="box-body">
            <div class="table-responsive">
<table id="FYName" class="table table-bordered table-striped">
<thead>
    <tr>
        
        <td>
            Financial Name
        </td>
        <td>
            Start Date
        </td>
        <td>
            End Date
        </td>
        <td>
            Company
        </td>
        <td>
            Branch
        </td>
        <td>Action</td>
    </tr>
    </thead>
    <tbody>
@foreach (var item in Model)
{
    <tr>
        
        <td>
            @item.FinancialName
        </td>
        <td>
            @String.Format("{0:g}", item.StartDate.ToString("dd-MMM-yyyy"))
        </td>
        <td>
            @String.Format("{0:g}", item.EndDate.ToString("dd-MMM-yyyy"))
        </td>
        <td>
            @item.CompanyName
        </td>
        <td>
            @item.BranchName
        </td>
        <td>
           <span data-toggle="tooltip" data-toggle="tooltip" title="Edit">
                                                @Html.ActionLink(" ", "FinancialYearEdit", new { id = item.FinancialYearId }, new { @class = "fa fa-pencil-square-o" })</span> 
                                                @*| <span data-toggle="tooltip" title="Delete">
                                                @Html.ActionLink(" ", "Delete", new { id = item.FinancialYearId }, new { @class = "fa fa-trash" })
                                                </span>*@
           
        </td>
    </tr>
}
</tbody>
<tfoot></tfoot>
</table>
</div>
</div>
</div>
</div>
</section>
    </div>
</div>
<script type="text/jscript">
    $(document).ready(function () {
        $('.back').click(function () {
            parent.history.back();
            return false;
        });
    });

    function goBack() {
        window.history.back();
    }
</script>
<script type="text/jscript">
    $(function () {

        $("#FYName").DataTable();

    });
</script>
