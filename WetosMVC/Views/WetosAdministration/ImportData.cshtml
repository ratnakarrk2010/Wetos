@* -- Added by Shalaka on 18th July 2017 for disabling Import Data Create -- *@
<script type="text/javascript">

    $(document).ready(function () {
        var EmployeeListCountId = +$("#EmployeeListCountId").val();

        //-- EmployeeListCount
        if (EmployeeListCountId > 0) {
            $("#ShiftBtn").prop("disabled", false);
        }
        else {

            $("#ShiftBtn").prop("disabled", true);
        }

        //-- ShiftCount
        var ShiftCountId = +$("#ShiftCountId").val();

        if (ShiftCountId > 0) {

            $("#LeaveBtn").prop("disabled", false);
            $("#LeaveCreditBtn").prop("disabled", false);
            $("#LeaveBalanceBtn").prop("disabled", false);
        }
        else {
            $("#LeaveBtn").prop("disabled", true);
            $("#LeaveCreditBtn").prop("disabled", true);
            $("#LeaveBalanceBtn").prop("disabled", true);
        }

        //-- LeaveMasterListCountId

        var LeaveMasterListCountId = +$("#LeaveMasterListCountId").val();
        if (LeaveMasterListCountId > 0) {
            $("#LeaveCreditBtn").prop("disabled", false);
            $("#LeaveBalanceBtn").prop("disabled", false);
        }
        else {
            $("#LeaveCreditBtn").prop("disabled", true);
            $("#LeaveBalanceBtn").prop("disabled", true);
        }

    });
</script>
