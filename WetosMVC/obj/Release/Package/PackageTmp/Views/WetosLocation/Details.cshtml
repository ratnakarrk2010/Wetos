          <div class="box">
            <div class="box-header">
             
            </div>
            <!-- /.box-header -->
            <div class="box-body">


<fieldset>
   <div class="row">
    <div class="display-label col-md-4">
        Location Name</div>
    <div class="display-field col-md-8">@Model.LocationName</div>
    </div>
    
     <div class="row">
    <div class="display-label col-md-4">
        Address</div>
    <div class="display-field col-md-8">@Model.Address</div>
    </div>
    
     <div class="row">
    <div class="display-label col-md-4">
        City</div>
    <div class="display-field col-md-8">@Model.City</div>
    </div>
    
</fieldset>
<p>
    @*@Html.ActionLink("Back to Location list", "Index")*@
    <span  class="btn btn-default" style="cursor:pointer; float:left" title = "Click to edit location details">@Html.ActionLink("Edit", "Edit", new { id = Model.LocationId })</span>
    <span  class="btn btn-default" style="cursor:pointer; float:left; margin-left:10px" title = "Click to go back to location details">@Html.ActionLink("Back to Location list", "Index")</span>
</p>

