                        @if (LBItem.TotalPaidLeaves == 0)
                        {
                            PendingLeaves = Convert.ToDouble(LBItem.TotalDeductableDays) - Convert.ToDouble(LBItem.TotalPaidLeaves);
                        }
                        
                        @{}
