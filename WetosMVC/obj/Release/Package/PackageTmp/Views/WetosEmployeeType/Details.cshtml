@model WetosDB.EmployeeType
@{
    ViewBag.Title = "Details";
}
<div class="page-content-wrapper" id="page_content">
    <div class="page-content">
        <section class="content-header">
<h1>Employee Type
<small>Details</small>
</h1>
<div class="row">
                <div class="col-md-12">
                    <ul class="page-breadcrumb breadcrumb">
                        <li>
                            <i class="fa fa-home"></i>
                            <a href="@Url.Action("Index", "Home")">Home</a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li>
                         <a href="#">Masters</a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "WetosEmployeeType")">Employee Type</a>
                        </li>
                        <li class="pull-right">
                            <div id="dashboard-report"  data-toggle="tooltip" title="Today's date">
                                <span style="cursor:pointer" data-toggle="tooltip"  class="back " title="Back" id="goback"><i class="fa fa-arrow-left"></i></span>
                                <i class="fa fa-calendar"></i>
                                <span>@DateTime.Now.ToLongDateString()</span>                           
                            </div>
                        </li>              
                    </ul>
                </div>
            </div>  
    <div style="clear: both;">
    </div>
      </section>
        <section class="content">
      <div class="row">
        <div class="col-xs-12 col-md-12 col-lg-12">
      <div class="box">
            <div class="box-header">
              
            </div>
            <!-- /.box-header -->
            <div class="box-body">
<div class="row">
    <div class="display-label col-md-4">EmployeeTypeName</div>
    <div class="display-field col-md-8">@Model.EmployeeTypeName</div>
    </div> 

        <br />
<p>
   <span style="cursor:pointer; float:left" title = "Click to edit Employee type" class="btn btn-default" >@Html.ActionLink("Edit", "Edit", new { id = Model.EmployeeTypeId })</span>
             <span  class="btn btn-default" style="cursor:pointer; float:left; margin-left:10px" title = "Click to go back to employee type list">@Html.ActionLink("Back to Employee type list", "Index")</span>
</p>


            <!-- /.box-body -->
          </div>
          <!-- /.box -->

          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    </div>
</div>
