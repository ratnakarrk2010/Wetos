@model WetosDB.VwBranchDetail
@{
    ViewBag.Title = "Details";
}
<div class="page-content-wrapper" id="page_content">
    <div class="page-content">
        <section class="content-header">
<h1>Branch
<small>Details</small>
</h1>
              <div class="row">
                <div class="col-md-12">
                    <ul class="page-breadcrumb breadcrumb">
                        <li>
                            <i class="fa fa-home"></i>
                            <a href="@Url.Action("Index", "Home")">Home </a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li>
                         <a href="#">Masters</a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "WetosBranch")">Branch </a>
                        </li>
                        <li class="pull-right">
                            <div id="dashboard-report"  data-toggle="tooltip" title="Today's date">
                                <span style="cursor: pointer" class="back "  data-toggle="tooltip" title="Back" title="Back" id="goback"><i class="fa fa-arrow-left"></i>@*<a href="#" >Back</a>*@
                                </span>
                                <i class="fa fa-calendar"></i>
                                <span>@DateTime.Now.ToLongDateString()
                                </span>
                            </div>
                            @* <span style="float:left">@Html.ActionLink("Add New Location", "Create")</span>*@
                        </li>
                    </ul>
                </div>
            </div>
      <div style="clear: both;"></div>

       @*Added by Pushkar For back breadcrumb on 24 OCT 2016-----------End*@



      </section>
        <section class="content">
      <div class="row">
        <div class="col-xs-12 col-md-12 col-lg-12">
      <div class="box">
            <div class="box-header">
              
            </div>
            <!-- /.box-header -->
            <div class="box-body">
            @using (Html.BeginForm("Delete", "WetosBranch", FormMethod.Post, new { id = "DeleteBranchForm" }))
            {
                @Html.ValidationSummary(true)

   
   <div class="row">
    <div class="display-label col-md-4">Branch Name</div>
    <div class="display-field col-md-8">@Model.BranchName</div>
  </div> 

        <br />
    <div class="row">
    <div class="display-label col-md-4">Company Name</div>
    <div class="display-field col-md-8">@Model.CompanyName</div>
    </div> 

        <br />
    <div class="row">
    <div class="display-label col-md-4">Location Name</div>
    <div class="display-field col-md-8">@Model.LocationName</div>
    </div> 

        <br />
   <div class="row">
    <div class="display-label col-md-4">Address1</div>
    <div class="display-field col-md-8">@Model.Address1</div>
    </div> 

        <br />
    <div class="row">
    <div class="display-label col-md-4">Address2</div>
    <div class="display-field col-md-8">@Model.Address2</div>
    </div> 

        <br />
    <div class="row">
    <div class="display-label col-md-4">Telephone</div>
    <div class="display-field col-md-8">@Model.Telephone</div>
    </div> 

        <br />
    <div class="row">
    <div class="display-label col-md-4">FAX</div>
    <div class="display-field col-md-8">@Model.FAX</div>
    </div> 

        <br />
    <div class="row">
    <div class="display-label col-md-4">Email</div>
    <div class="display-field col-md-8">@Model.Email</div>
    </div> 

        <br />


<p>
<input type="button" value="Delete" id="Deletebranch" style="cursor:pointer; float:left" title = "Click to delete Branch" class="btn btn-default" />
 <span  class="btn btn-default" style="cursor:pointer; float:left; margin-left:10px" title = "Click to go back to branch list">@Html.ActionLink("Back to Branch list", "Index")</span>
    
   
</p>


            <!-- /.box-body -->
            }
          </div>
            
          <!-- /.box -->

          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    </div>
</div>
@*ADDED CODE BY RAJAS ON 28 MARCH 2017 FOR ALERT POPUP START*@
<div id="branchdiv" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-body">
                Do you want to delete this Branch?
            </div>
            <div class="modal-footer">
                <button type="button" data-dismiss="modal" class="col-md-2  btn btn-primary" id="DeleteBtn">
                    Yes</button>
                <button type="button" data-dismiss="modal" class="col-md-2 btn">
                    Cancel</button>
            </div>
        </div>
    </div>
</div>
@*ADDED CODE BY RAJAS ON 28 MARCH 2017 FOR ALERT POPUP END*@ @*Added by Pushkar For back breadcrumb on 24 OCT 2016-------start*@
<script type="text/jscript">
    $(document).ready(function () {
        $('.back').click(function () {
            parent.history.back();
            return false;
        });
    });

    function goBack() {
        window.history.back();
    }

    //ADDED CODE BY RAJAS ON 28 MARCH 2017 FOR ALERT POPUP START
    $("#Deletebranch").click(function () {

        var $form = $(this).closest('form');
        $('#branchdiv').modal({ backdrop: 'static', keyboard: false })
                .one('click', '#DeleteBtn', function () {

                    $("#DeleteBranchForm").submit();
                });

    });
    //ADDED CODE BY RAJAS ON 28 MARCH 2017 FOR ALERT POPUP END


</script>
@*Added by Pushkar For back breadcrumb on 24 OCT 2016---------end*@
