        var Branchid = $("#BranchddlList").val();

        var EmployeeTypeId = null;

        var FYID = $("#FYid").val();
        var MonthId = $("#Month").val();
        var Fromdate = $("#FromDate").val();
        var Todate = $("#ToDate").val();

