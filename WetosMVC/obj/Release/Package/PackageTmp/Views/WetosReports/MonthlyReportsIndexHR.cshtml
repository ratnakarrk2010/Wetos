<script type="text/javascript">
    $(function () {
        //Initialize Select2 Elements
        $(".select2").select2();
        //$(".js-example-basic-multiple").select2();
        //Datemask dd/mm/yyyy


        //Date picker
        $('.datepicker').datepicker({
            format: "dd-M-yyyy",
            autoclose: true
        });

    });


    // Added by Rajas on 27 DEC 2016 to fill drop down list on selection of other drop down
    $("#CompanyddlList").change(function () {

        //alert("GET Branch");

        isDirty = false;
        var companyid = +$("#CompanyddlList").val();

        $.ajax({
            url: '@Url.Content("~/WetosReports/GetBranch")',
            type: "POST",
            dataType: "json",
            data: { Companyid: companyid },
            success: function (data) {
                $("#BranchddlList").empty();

                $("#BranchddlList").append("<option value=" + "0" + ">" + "Select" + "</option>");

                for (var i = 0; i < data.length; i++) {
                    $("#BranchddlList").append("<option value=" + "" + data[i].BranchId + "" + ">" + data[i].BranchName + "</option>");
                }
                SelectEmployeeList();
            },
            error: function (data) {
                //                alert("ERROR");
                //                alert(data);

                for (var i = 0; i < data.length; i++) {

                    // alert(states[i].Group_name);
                    // states.option.selected = true;
                    $("#BranchddlList").append("<option value=" + "" + data[i].BranchId + "" + ">" + data[i].BranchName + "</option>");
                    //$("#DepartmentddlList").append("<option value=" + "" + data[i].DepartmentId + "" + ">" + data[i].DepartmentName + "</option>");
                    //SelectEmployeeList();
                }
            }
        });

    });

    $("#BranchddlList").change(function () {

        //alert("GET DEPARTMENT");
        // alert($("#Group_code").val());
        isDirty = false;
        var branchid = +$("#BranchddlList").val();
        $.ajax({
            url: '@Url.Content("~/WetosReports/GetDepartment")',
            type: "POST",
            dataType: "json",
            data: { Branchid: branchid },
            success: function (data) {

                $("#DepartmentId").empty();

                $("#DepartmentId").append("<option value=" + "0" + ">" + "Select" + "</option>");
                if (data.length == 0) {
                    SelectEmployeeList();
                }
                else {
                    for (var i = 0; i < data.length; i++) {

                        //alert(states[i].Group_name);
                        // states.option.selected = true;
                        $("#DepartmentId").append("<option value=" + "" + data[i].DepartmentId + "" + ">" + data[i].DepartmentName + "</option>");

                        //SelectEmployeeList(); // COMMNETED BY MSJ ON 27 DEC 2017 
                    }
                    SelectEmployeeList();
                }
            },
            error: function (data) {
                //alert("hello");
            }
        });
    });

    $("#DepartmentId").change(function () {
        //alert("GET EMPLOYEE");

        //var DepartmentIdList = $("#DepartmentddlList").val();
        // alert(DepartmentIdList);

        var Companyid = $("#CompanyddlList").val();  // Used +$ to handle null values, as Companyid of type int 
