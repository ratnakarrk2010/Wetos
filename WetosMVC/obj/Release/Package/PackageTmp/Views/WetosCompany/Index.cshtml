@model IEnumerable<WetosDB.VwLocationNameInCompany>
@{
    ViewBag.Title = "Company Master List";
                     //Layout = null;
}
@System.Web.Optimization.Styles.Render("~/Content/DataTable")
 @System.Web.Optimization.Scripts.Render("~/bundles/DataTable")

<link href="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js" type="text/javascript" />
<link href="https://cdn.datatables.net/buttons/1.5.1/js/dataTables.buttons.min.js"
    type="text/javascript" />
<link href="https://cdn.datatables.net/buttons/1.5.1/js/buttons.flash.min.js" type="text/javascript" />
<link href="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js" type="text/javascript" />
<link href="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.32/pdfmake.min.js"
    type="text/javascript" />
<link href="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.32/vfs_fonts.js" type="text/javascript" />
<link href="https://cdn.datatables.net/buttons/1.5.1/js/buttons.html5.min.js" type="text/javascript" />
<link href="https://cdn.datatables.net/buttons/1.5.1/js/buttons.print.min.js" type="text/javascript" />
<link href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" type="text/css" />
<link href="https://cdn.datatables.net/buttons/1.5.1/css/buttons.dataTables.min.css"
    type="text/css" />
<div class="page-content-wrapper" id="page_content">
    <div class="page-content">
        <section class="content-header">
            <h1>Company Master
                <small>List</small>
                 <span data-toggle="tooltip" title="Add Company" style="float:right;font-weight:800" onclick="OnAddCompanyClick();">
                 @Html.ActionLink(" ", "Create", new { }, new { @class = "fa fa-plus-circle", title = "Add Company", style = "color:#428bca;font-size:19px" })</span>
            </h1>
            <div class="row">
                <div class="col-md-12">
                    <ul class="page-breadcrumb breadcrumb">
                        <li>
                            <i class="fa fa-home"></i>
                            <a href="@Url.Action("Index", "Home")">Home </a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li>
                         <a href="#">Masters</a>
                            <i class="fa fa-angle-right"></i>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "WetosCompany")">Company </a>
                        </li>
                        <li class="pull-right">
                            <div id="dashboard-report"  data-toggle="tooltip" title="Today's date">
                                <span style="cursor:pointer" class="back "  data-toggle="tooltip" title="Back" id="goback"><i class="fa fa-arrow-left"></i></span>
                                <i class="fa fa-calendar"></i>
                                <span>@DateTime.Now.ToLongDateString()</span>                           
                            </div>
                        </li>              
                    </ul>
                </div>
            </div>  
           
        </section>
        <section class="content">
           @* <div>      
                <span class="btn btn-default" style="float:left;font-weight:200; cursor:pointer" "tooltip" title = "Click here to add new company">@Html.ActionLink("Add New Company", "Create")</span>
            </div>   
            *@
            
            <div class="row">
                <div class="col-xs-12">
                    <div class="box">
              
                            <div class="box-body">
                           
                                <table id="CompanyMaster" class="table table-bordered table-striped">  @*Added by Pushkar For datatable on 25 OCT 2016*@
                                    <thead>
                                        <tr>
                                            <td>
                                                Company
                                            </td>
                                            <td>
                                                Location
                                            </td>
                                            <td>
                                                Address
                                            </td>
                                            <td>
                                                City
                                            </td>
                                            @*   <td>
                                                Telephone
                                            </td>*@
                                            <td>
                                                Email
                                            </td>
                                            <td>
                                                Fax
                                            </td>
                                            @*  <td>
                                                PAN
                                            </td>
                                            <td>
                                                CST
                                            </td>
                                            <td>
                                                EXC
                                            </td>
                                            <td>
                                                TDS
                                            </td>
                                            <td>
                                                SGNNM
                                            </td>
                                            <td>
                                                SGNPlace
                                            </td>*@
                                            <td>PIN Code</td>
                                            <td>Action</td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                          <tr>      
                                            <td>
                                            <a href="@Url.Action("Details", "WetosCompany", new { id = item.CompanyId })" style="cursor:pointer; float:left" title = "Click to check company details">
                                                @item.CompanyName</a>
                                            </td>
                                            <td>
                                                @item.LocationName
                                            </td>
                                            <td>
                                                @item.Address1
                                            </td>
                                            <td>
                                                @item.Address2
                                            </td>
                                          @*  <td>
                                                @item.Telephone
                                            </td>*@
                                            <td>
                                                @item.Email
                                            </td>
                                            <td>
                                                @item.Fax
                                            </td>
                                           @* <td>
                                                @item.PAN
                                            </td>
                                            <td>
                                                @item.CST
                                            </td>
                                            <td>
                                                @item.EXC
                                            </td>
                                            <td>
                                                @item.TDS
                                            </td>
                                            <td>
                                                @item.SGNNM
                                            </td>
                                            <td>
                                                @item.SGNPlace
                                            </td>*@
                                            @*<td></td>*@
                                            @*Above Line Commented and below line is added by Shalaka on 18th DEC 2017*@
                                            <td>@item.PINCode</td>
                                              <td>
                                              <span data-toggle="tooltip" title="Edit">
                                                @Html.ActionLink(" ", "Edit", new { id = item.CompanyId }, new { @class = "fa fa-pencil-square-o" })</span> |
                                                @*@Html.ActionLink("Details", "Details", new { id = item.CompanyId }) |*@
                                                <span data-toggle="tooltip" title="Delete">
                                                @Html.ActionLink(" ", "Delete", new { id = item.CompanyId }, new { @class = "fa fa-trash" })
                                                </span>
                                            </td>
                                        </tr>
                                        }
                                    </tbody>
                                <tfoot></tfoot>
                            </table>
                        </div>
                        <!-- /.box-body -->
                    </div>                    
                    <!-- /.box -->
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </section>
    </div>
</div>
