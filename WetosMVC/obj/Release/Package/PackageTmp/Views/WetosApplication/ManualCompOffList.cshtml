@model IEnumerable<WetosDB.SP_GetManualCompOffList_Result>
@{
    ViewBag.Title = "ManualCompOffList";
}
<div class="page-content-wrapper" id="page_content">
    <div class="page-content">
        <section class="content-header">
            <h1>Manual Comp Off
                <small>List</small>
                 <span class="btn blue" data-toggle="tooltip" title="Apply Manual Comp Off" style="float:right;font-weight:800" onclick="OnAddCompanyClick();">@Html.ActionLink(" ", "ManualCompOffApplicationIndex", new { }, new { @class = "fa fa-plus-square", style = "color:white" })</span>
            </h1>
            <div class="row">
                <div class="col-md-12">
                    <ul class="page-breadcrumb breadcrumb">
                        <li>
